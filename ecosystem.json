{
  "apps": [{
    "name": "test",
    "script": "test/fixtures/app.js"
  }],
  "deploy": {
    "development": {
      "user": "node",
      "host": "127.0.0.1",
      "ref": "origin/master",
      "repo": "https://github.com/gifff/grunt-pm2-deploy.git",
      "path": "~/.tmp/development",
      "post-deploy": "~/node_modules/pm2/bin/pm2 startOrRestart ecosystem.json --env development"
    },
    "production": {
      "user": "node",
      "host": "127.0.0.1",
      "ref": "origin/master",
      "repo": "https://github.com/gifff/grunt-pm2-deploy.git",
      "path": "~/.tmp/production",
      "post-deploy": "~/node_modules/pm2/bin/pm2 startOrRestart ecosystem.json --env production"
    }
  }
}
